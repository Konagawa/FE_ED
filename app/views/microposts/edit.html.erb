<%= provide(:title, "投稿編集") %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @post) do |f| %>

      <%= f.label :content, "投稿詳細" %>
      <%= f.text_area :content %>

      <%= f.submit "編集内容を保存する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
